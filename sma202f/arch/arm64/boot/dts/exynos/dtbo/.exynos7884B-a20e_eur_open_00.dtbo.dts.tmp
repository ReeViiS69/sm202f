/ {
 fragment@lcd {
  target-path = "/";
  __overlay__ {
   decon_board_hx83102d: decon_board_hx83102d {
    gpio_lcd_3p0 = <&gpg1 2 0>;
    gpio_lcd_rst = <&gpg2 0 0>;

    dsim_set_panel_power_early {
     type =
     "gpio,high", "gpio_lcd_3p0",
     "delay,usleep", "5000";
    };
    dsim_set_panel_power_disable {
     type =
     "delay,msleep", "34",
     "gpio,low", "gpio_lcd_rst",
     "delay,usleep", "5000";
    };
    dsim_reset_panel {
     type =
     "gpio,high", "gpio_lcd_rst",
     "delay,usleep", "1000",
     "gpio,low", "gpio_lcd_rst",
     "delay,usleep", "1000",
     "gpio,high", "gpio_lcd_rst",
     "delay,msleep", "60";
    };
   };

   hx83102d: hx83102d {
    mode = <0>;
    resolution = <720 1560>;
    size = <62 135>;
    timing,refresh = <60>;

    timing,h-porch = <26 150 12>;

    timing,v-porch = <12 254 2>;
    timing,dsi-hs-clk = <598>;
    timing,pms = <2 92 1>;
    timing,dsi-escape-clk = <16>;
    mic_en = <0>;
    mic_ratio = <0>;
    mic_ver = <0>;
    type_of_ddi = <0>;
    dsc_en = <0>;
    dsc_cnt = <0>;
    dsc_slice_num = <0>;
    data_lane = <4>;
    vt_compensation = <455>;
    eotp_disabled = <1>;
    decon_board = <&decon_board_hx83102d>;
   };
  };
 };
};

